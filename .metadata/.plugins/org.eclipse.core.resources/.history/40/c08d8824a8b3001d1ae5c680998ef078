package com.serverless.assignment.springbootawsserverless.model;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Getter
@Setter
@ToString
public class Product {
	private int id;
	private String name;
	private String discription;
	private int price;

	public Product(String json) {
		Gson gson = json.fromJson
	}
}
